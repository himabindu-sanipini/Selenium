<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel = "tests" name="Suite">
	<groups>
		<run>
			<include name="ErrorHandling"/>>
		</run>
	</groups>
  <test thread-count="5" name="ErrorValidations Test">
    <classes>
     <class name="EPAM.Selenium.ErrorValidations"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="SubmitOrderTest Test">
    <classes>
      <class name="EPAM.Selenium.SubmitOrderTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
